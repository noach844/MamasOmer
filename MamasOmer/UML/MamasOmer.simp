                              <p>
                                <t> Loop over employees and adds to table
 Output table
</t>
                              </p>
                            </paragraph>
                          </content>
                        </text>
                      </documentation>
                      <source-codes>
                        <source-code language="CSharp">
                           
            Console.Write("Enter name or part of it (keep empty to show all): ");
            string searchText = Console.ReadLine().ToLower();
                                       
            var result = Employees.Where(employee =&gt; employee.Name.ToLower().Contains(searchText)).ToList();
                                     
            printEmployeeTable(result);            
        </source-code>

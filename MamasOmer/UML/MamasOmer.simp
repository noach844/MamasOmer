                    <operation id="of706d4590b804715a7013a816f3d5ef3212" cid="OP034" name="GetEmployeeWage" creation-date="06/26/2021 17:45:26" type="" type-id="" visibility="public">
                      <documentation>
                        <text>
                          <default>
                            <style font-name="Verdana" font-size="12">
                              <text>
                                <solid-brush color="#FF000000" />
                              </text>
                            </style>
                          </default>
                          <styles />
                          <content>
                            <paragraph>
                              <style />
                              <p>
                                <t> Validate number input
 Get wanted employee by id using Linq
 Check if there are results
 Get month name as string
 Output employee's wage
 &lt;summary&gt;
  The function gets employee's id as input from user and insert card for specific employee.
  Works for start of shift and end of shift.
 &lt;/summary&gt;
 handle input
 validate number input
</t>
                              </p>
                            </paragraph>
                          </content>
                        </text>
                      </documentation>
                      <source-codes>
                        <source-code language="CSharp">
                            
            int id;
            Console.Write("Enter Employee's ID: ");
                                    
            while (true)
            {
                try
                {
                    id = Int32.Parse(Console.ReadLine());
                    break;
                }
                catch
                {
                    Console.WriteLine("Please enter a number!");
                }
            }
                                                   
            var result = Employees.Where(employee =&gt; employee.ID == id).ToList();
                                         
            if (result.Count &gt; 0)
            {
                Employee employee = result[0];
                                           
                DateTimeFormatInfo mfi = new DateTimeFormatInfo();                
                string month = mfi.GetMonthName(employee.Month);
                                         
                Console.WriteLine($"The wage of {employee.Name} is {employee.WageCalculator()} right to {month}");
            }
            else
            {
                Console.WriteLine($"No customer with ID:{id} found!");
            }
        </source-code>
                      </source-codes>
                    </operation>
                    <operation id="o4e8fe0c29fa44a83a2925fe596b3126d213" cid="OP035" name="InsertCard" creation-date="06/26/2021 17:45:26" type="" type-id="" visibility="public">
                      <documentation>

                    <operation id="o043be1ab0e7f48239149b306647f0bdd213" cid="OP037" name="printEmployeeTable" creation-date="06/26/2021 17:45:26" type="" type-id="" visibility="public">
                      <parameters>
                        <parameter id="p78435d3de509430b883e6910f07a2afa" name="employees" type="List&lt;Employee&gt;" type-id="List&lt;Employee&gt;" />
                      </parameters>
                      <source-codes>
                        <source-code language="CSharp">
                          
            var table = new ConsoleTable("ID", "Name", "Role");            
                                                    
            foreach(Employee employee in employees)
            {
                table.AddRow(employee.ID, employee.Name, employee.Role);
            }
                           
            table.Write();
